    "n = int(input())\n",
    "a = input()\n",
    "scores = a.split(\" \")\n",
    "\n",
    "for i in range(n):\n",
    "    scores[i] = int(scores[i])\n",
    "\n",
    "M = max(scores)\n",
    "\n",
    "for i in range(n):\n",
    "    scores[i] = scores[i]/M*100\n",
    "    \n",
    "print(sum(scores)/n)"
