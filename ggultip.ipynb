   "source": [
    "def jegop(x, y):\n",
    "    sq_sum = 0\n",
    "    n_sum = 0\n",
    "    sum_sq = 0\n",
    "    for i in range(x, y+1):\n",
    "        sq_sum += i**2\n",
    "        n_sum += i\n",
    "    sum_sq = n_sum**2\n",
    "\n",
    "    return sum_sq - sq_sum"
   ]
  }
